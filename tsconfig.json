{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      "@dtos/*":[
        "./src/dtos/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
      "@services/*":[
        "./src/services/*"
      ],
      "@hooks/*":[
        "./src/hooks/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ]
    }
  }
}
